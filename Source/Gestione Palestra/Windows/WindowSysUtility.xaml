<Window x:Class="GestionePalestra.WindowSysUtility"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:xtk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:local="clr-namespace:GestionePalestra"
        mc:Ignorable="d"
        Height="696.999" Width="720.428" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="button_Copy" Content="Stampa" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Width="75" Click="button_Copy_Click"/>
