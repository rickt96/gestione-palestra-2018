            <Button.Template>
                <ControlTemplate TargetType="Button">
                    <Grid>
                        <Ellipse Stroke="#FF006DF0"  StrokeThickness="1">
                            <Ellipse.Fill>
                                <ImageBrush x:Name="img_profilo" Stretch="UniformToFill" ImageSource="/Gestione Palestra;component/Resources/Icons/BlueLogin.png" />
                            </Ellipse.Fill>
                        </Ellipse>
                    </Grid>
                </ControlTemplate>
            </Button.Template>
        </Button>
        <Button x:Name="button" Content="clienti dialog" HorizontalAlignment="Left" Margin="140,60,0,0" VerticalAlignment="Top" Width="75" Click="button_Click_3"/>
