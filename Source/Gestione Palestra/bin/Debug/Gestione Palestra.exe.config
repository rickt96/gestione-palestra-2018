<?xml version="1.0" encoding="utf-8"?>
<configuration>
  
  <configSections></configSections>
  
  <startup> 
      <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="MySql.Data" publicKeyToken="c5687fc88969c44d" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.9.12.0" newVersion="6.9.12.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

  <system.data>
    <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.9.12.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories>
  </system.data>

  <appSettings>
    
    <!--parametri connessione db-->
    <add key="DB_HOST" value="localhost"/>
    <add key="DB_USER" value="root"/>
    <add key="DB_PASSWORD" value="raspberry"/>
    <add key="DB_NAME" value="itrainerDB"/>
    <add key="DB_PORT" value="3307"/>
    
    <!--parametri impostazioni app-->
    <add key="ACTIVITY_NAME" value="il mio centro"/>
    <add key="STARTUP_DATE" value="10/05/2017 00:00:00"/>
    <add key="ERROR_LOGGER_ENABLED" value="False"/>
    <add key="CHANGES_LOGGER_ENABLED" value="False"/>
    <add key="BACKUP_TYPE" value="4"/>
    <add key="LAST_RESTORE_DATE" value="17/09/2018 03:56:00"/>
    <add key="CHROME_PATH" value="C:\Program Files (x86)\Google\Chrome\Application\chrome.exe"/>

  </appSettings>

</configuration>